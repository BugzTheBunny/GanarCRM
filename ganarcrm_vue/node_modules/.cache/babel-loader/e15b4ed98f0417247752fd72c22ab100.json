;AAAA;AAAA;AAef;AAhBM,GAVI;AA4BP,EAAA,eA5BO,6BA4BW;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACtB,cAAA,MAAI,CAAC,MAAL,CAAY,MAAZ,CAAmB,cAAnB,EAAmC,IAAnC;;AACM,cAAA,MAFgB,GAEP,MAAI,CAAC,MAAL,CAAY,MAAZ,CAAmB,EAFZ;AAGhB,cAAA,IAHgB,GAGT;AACX,gBAAA,OAAO,EAAE;AADE,eAHS;AAAA;AAAA,qBAMhB,KAAI,CACP,IADG,oCAEH,IAFG,CAEE,UAAC,QAAD,EAAc;AAClB,gBAAA,MAAI,CAAC,IAAL,GAAY,QAAQ,CAAC,IAArB;AACD,eAJG,EAKH,KALG,CAKG,UAAC,KAAD,EAAW;AAChB,gBAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,eAPG,CANgB;;AAAA;AActB,cAAA,MAAI,CAAC,MAAL,CAAY,MAAZ,CAAmB,cAAnB,EAAmC,KAAnC;;AAdsB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAevB;AA3CY,CAAf","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"columns is-multiline\">\r\n      <div class=\"column is-12\">\r\n        <h1 class=\"title\">{{ lead.company }}</h1>\r\n        <div class=\"buttons\">\r\n          <router-link\r\n            :to=\"{ name: 'EditLead', params: { id: $route.params.id } }\"\r\n            class=\"button is-success\"\r\n            >Edit</router-link\r\n          >\r\n          <button class=\"button is-warning\" @click=\"convertToClient\">\r\n            Convert to client\r\n          </button>\r\n        </div>\r\n      </div>\r\n      <div class=\"column is-6\">\r\n        <div class=\"box\">\r\n          <h2 class=\"subtitle\">Details:</h2>\r\n          <template v-if=\"lead.assigned_to\">\r\n            <p>\r\n              <strong>Assigned to: </strong>\r\n              {{ lead.assigned_to.first_name }}\r\n              {{ lead.assigned_to.last_name }}\r\n            </p>\r\n          </template>\r\n          <p><strong>Status: </strong>{{ lead.status }}</p>\r\n          <p><strong>Priority: </strong>{{ lead.priority }}</p>\r\n          <p><strong>Confidence: </strong>{{ lead.confidence }}</p>\r\n          <p><strong>Estimated Value: </strong>{{ lead.estimated_value }}</p>\r\n          <p><strong>Created at: </strong>{{ lead.created_at }}</p>\r\n          <p><strong>Modified at: </strong>{{ lead.modified_at }}</p>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"column is-6\">\r\n        <div class=\"box\">\r\n          <h2 class=\"subtitle\">Contact Information:</h2>\r\n\r\n          <p><strong>Contact Person: </strong>{{ lead.contact_person }}</p>\r\n          <p><strong>Email: </strong>{{ lead.email }}</p>\r\n          <p><strong>Phone: </strong>{{ lead.phone }}</p>\r\n          <p><strong>Website: </strong>{{ lead.website }}</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Lead\",\r\n  data() {\r\n    return {\r\n      lead: {},\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getLead();\r\n  },\r\n  methods: {\r\n    async getLead() {\r\n      this.$store.commit(\"setIsLoading\", true);\r\n\r\n      const leadId = this.$route.params.id;\r\n\r\n      await axios\r\n        .get(`/api/v1/leads/${leadId}/`)\r\n        .then((response) => {\r\n          this.lead = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n\r\n      this.$store.commit(\"setIsLoading\", false);\r\n    },\r\n  },\r\n  async convertToClient() {\r\n    this.$store.commit(\"setIsLoading\", true);\r\n    const leadId = this.$route.params.id;\r\n    const data = {\r\n      lead_id: leadID,\r\n    };\r\n    await axios\r\n      .post(`/api/v1/convert_lead_to_client/`)\r\n      .then((response) => {\r\n        this.lead = response.data;\r\n      })\r\n      .catch((error) => {\r\n        console.log(error);\r\n      });\r\n    this.$store.commit(\"setIsLoading\", false);\r\n  },\r\n};\r\n</script>"],"sourceRoot":""}]}