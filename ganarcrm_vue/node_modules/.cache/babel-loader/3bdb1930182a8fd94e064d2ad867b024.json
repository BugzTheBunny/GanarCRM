AI,CAAC,aAAL,GAAqB,QAAQ,CAAC,IAAT,CAAc,KAAnC;AACD,iBAJG,EAKH,KALG,CAKG,UAAC,KAAD,EAAW;AAChB,kBAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,iBAPG,CAHW;;AAAA;AAAA;AAAA,uBAYX,KAAI,CACP,GADG,iCAC0B,KAAI,CAAC,WAD/B,qBACqD,KAAI,CAAC,KAD1D,GAEH,IAFG,CAEE,UAAC,QAAD,EAAc;AAClB,kBAAA,KAAI,CAAC,OAAL,GAAe,QAAQ,CAAC,IAAT,CAAc,OAA7B;;AAEA,sBAAI,QAAQ,CAAC,IAAT,CAAc,IAAlB,EAAwB;AACtB,oBAAA,KAAI,CAAC,cAAL,GAAsB,IAAtB;AACF,mBAFA,MAEO;AACL,oBAAA,KAAI,CAAC,cAAL,GAAsB,KAAtB;AACF;;AAEA,sBAAI,QAAQ,CAAC,IAAT,CAAc,QAAlB,EAA4B;AAC1B,oBAAA,KAAI,CAAC,kBAAL,GAA0B,IAA1B;AACF,mBAFA,MAEO;AACL,oBAAA,KAAI,CAAC,kBAAL,GAA0B,KAA1B;AACF;AACD,iBAhBG,EAiBH,KAjBG,CAiBG,UAAC,KAAD,EAAW;AAChB,kBAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,iBAnBG,CAZW;;AAAA;AAiCjB,gBAAA,KAAI,CAAC,MAAL,CAAY,MAAZ,CAAmB,cAAnB,EAAmC,KAAnC;;AAjCiB;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAkClB,KA5CM;AA6CP,IAAA,WA7CO,yBA6CO;AACZ,WAAK,UAAL;AACD;AA/CM;AAjBI,CAAf","sourcesContent":["<template >\r\n  <div class=\"container\">\r\n    <div class=\"columns is-multiline\">\r\n      <div class=\"column is-8\">\r\n        <h1 class=\"title\">Clients</h1>\r\n      </div>\r\n      <div class=\"column is-4\">\r\n        <div class=\"buttons\">\r\n          <router-link\r\n            v-if=\"$store.state.team.max_clients > total_clients\"\r\n            class=\"button is-info\"\r\n            :to=\"{ name: 'AddClient' }\"\r\n            >Add Client\r\n          </router-link>\r\n          <div class=\"notification is-danger\" v-else>\r\n            Max clients amount reached, please upgrade plan.\r\n          </div>\r\n          <form @submit.prevent=\"filterTable\">\r\n            <div class=\"field has-addons\">\r\n              <div class=\"control\">\r\n                <input\r\n                  class=\"input\"\r\n                  type=\"text\"\r\n                  placeholder=\"Client..\"\r\n                  v-model=\"query\"\r\n                />\r\n              </div>\r\n              <div class=\"control\">\r\n                <button class=\"button is-success\">Search</button>\r\n              </div>\r\n            </div>\r\n          </form>\r\n        </div>\r\n      </div>\r\n      <div class=\"column is-12\">\r\n        <div class=\"buttons\">\r\n          <button\r\n            class=\"button is-light\"\r\n            v-if=\"showPreviousButton\"\r\n            @click=\"goToPreviousPage()\"\r\n          >\r\n            <strong> Previous </strong>\r\n          </button>\r\n          <button\r\n            class=\"button is-light\"\r\n            v-if=\"showNextButton\"\r\n            @click=\"goToNextPage()\"\r\n          >\r\n            <strong>Next</strong>\r\n          </button>\r\n        </div>\r\n        <template v-if=\"clients.length\">\r\n          <table class=\"table is-fullwidth\">\r\n            <thead>\r\n              <tr>\r\n                <th>Name</th>\r\n                <th>Contact person</th>\r\n                <th></th>\r\n              </tr>\r\n            </thead>\r\n            <tbody>\r\n              <tr v-for=\"client in clients\" v-bind:key=\"client.id\">\r\n                <td>{{ client.name }}</td>\r\n                <td>{{ client.contact_person }}</td>\r\n                <td>\r\n                  <div class=\"buttons\">\r\n                    <router-link\r\n                      :to=\"{ name: 'Client', params: { id: client.id } }\"\r\n                      class=\"button is-success\"\r\n                      >View</router-link\r\n                    >\r\n                    <router-link\r\n                      :to=\"{ name: 'EditClient', params: { id: client.id } }\"\r\n                      class=\"button is-success\"\r\n                      >Edit</router-link\r\n                    >\r\n                  </div>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </template>\r\n        <template v-else>\r\n          <h1>You don't have any clients yet.</h1>\r\n        </template>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Clients\",\r\n  data() {\r\n    return {\r\n      clients: [],\r\n      total_clients: 0,\r\n      showNextButton: false,\r\n      showPreviousButton: false,\r\n      currentPage: 1,\r\n      query: \"\",\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    this.getClients();\r\n  },\r\n\r\n  methods: {\r\n    goToNextPage() {\r\n      this.currentPage += 1;\r\n      this.getClients();\r\n    },\r\n\r\n    goToPreviousPage() {\r\n      this.currentPage -= 1;\r\n      this.getClients();\r\n    },\r\n    async getClients() {\r\n      this.$store.commit(\"setIsLoading\", true);\r\n\r\n      await axios\r\n        .get(`/api/v1/clients/`)\r\n        .then((response) => {\r\n          this.total_clients = response.data.count;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n\r\n      await axios\r\n        .get(`/api/v1/clients/?page=${this.currentPage}&search=${this.query}`)\r\n        .then((response) => {\r\n          this.clients = response.data.results;\r\n\r\n          if (response.data.next) {\r\n            this.showNextButton = true;\r\n          } else {\r\n            this.showNextButton = false;\r\n          }\r\n\r\n          if (response.data.previous) {\r\n            this.showPreviousButton = true;\r\n          } else {\r\n            this.showPreviousButton = false;\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n\r\n      this.$store.commit(\"setIsLoading\", false);\r\n    },\r\n    filterTable() {\r\n      this.getClients();\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":""}]}