         <p><strong>Phone: </strong>{{ client.phone }}</p>\r\n          <p><strong>Website: </strong>{{ client.website }}</p>\r\n        </div>\r\n      </div>\r\n      <hr />\r\n\r\n      <div class=\"column is-12\">\r\n        <h2 class=\"subtitle\">Notes</h2>\r\n        <router-link\r\n          :to=\"{ name: 'AddNote', params: { id: $route.params.id } }\"\r\n          class=\"button is-success\"\r\n          >Add Note</router-link\r\n        >\r\n        <div class=\"box mt-4\" v-for=\"note in notes\" v-bind:key=\"note.id\">\r\n          <h3 class=\"is-size-4\">{{ note.name }}</h3>\r\n          <p>{{ note.body }}</p>\r\n          <router-link\r\n            :to=\"{\r\n              name: 'EditNote',\r\n              params: { id: $route.params.id, note_id: note.id },\r\n            }\"\r\n            class=\"button is-success\"\r\n            >Edit Note</router-link\r\n          >\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Client\",\r\n  data() {\r\n    return {\r\n      client: {},\r\n      notes: [],\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getClient();\r\n  },\r\n  methods: {\r\n    async getClient() {\r\n      this.$store.commit(\"setIsLoading\", true);\r\n\r\n      const clientId = this.$route.params.id;\r\n\r\n      await axios\r\n        .get(`/api/v1/clients/${clientId}/`)\r\n        .then((response) => {\r\n          this.client = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n\r\n      await axios\r\n        .get(`/api/v1/notes/?client_id=${clientId}`)\r\n        .then((response) => {\r\n          this.notes = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n\r\n      this.$store.commit(\"setIsLoading\", false);\r\n    },\r\n  },\r\n};\r\n</script>\r\n"],"sourceRoot":""}]}