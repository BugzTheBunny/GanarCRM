     <router-link class=\"button is-info\" to=\"/dashboard/leads/add\">Add Lead </router-link>\r\n\r\n            </div>\r\n            <div class=\"column is-12\">\r\n                <table class=\"table is-fullwidth\">\r\n                    <thead>\r\n                        <tr>\r\n                            <th>Company</th>\r\n                            <th>Contact Person</th>\r\n                            <th>Status</th>\r\n                            <th></th>\r\n                        </tr>\r\n                    </thead>\r\n                    <tbody>\r\n                        <tr\r\n                        v-for=\"lead in leads\"\r\n                        v-bind:key=\"lead.id\"\r\n                        >\r\n                        <td>{{lead.company}}</td>\r\n                        <td>{{lead.contact_person}}</td>\r\n                        <td>{{lead.status}}</td>\r\n                        <td>\r\n                            <div class=\"buttons\">\r\n                            <router-link :to=\"{ name:'Lead' ,params: {id:lead.id}}\" class=\"button is-success\">View</router-link>\r\n                            <router-link :to=\"{ name:'EditLead' ,params: {id:lead.id}}\" class=\"button is-success\">Edit</router-link>\r\n                            </div>\r\n                        </td>\r\n                        </tr>\r\n                    </tbody>\r\n                </table>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\n    import axios from 'axios'\r\n\r\n\r\n    export default {\r\n        name: 'Lead',\r\n        data(){\r\n            return {\r\n                leads:[]\r\n            }\r\n        },\r\n\r\n        mounted(){\r\n            this.getLeads()\r\n        },\r\n\r\n        methods:{\r\n            async getLeads(){\r\n                this.$store.commit('setIsLoading',true)\r\n\r\n                axios\r\n                    .get('/api/v1/leads/')\r\n                    .then(response => {\r\n                        this.leads = response.data\r\n                    }).catch(error => {\r\n                        console.log(error)\r\n                    })\r\n\r\n                this.$store.commit('setIsLoading',false)\r\n\r\n}\r\n        }\r\n\r\n    }\r\n</script>"],"sourceRoot":""}]}