,EALJ,mBAAA,CAAqD,GAArD,EAAqD,IAArD,EAAqD,CAAlD,WAAkD,E,kCAApB,KAAA,CAAA,IAAA,CAAK,Q,GAAQ;AAAA;GAAO,CAArD,CAKI,EAJJ,mBAAA,CAAyD,GAAzD,EAAyD,IAAzD,EAAyD,CAAtD,WAAsD,E,kCAAtB,KAAA,CAAA,IAAA,CAAK,U,GAAU;AAAA;GAAO,CAAzD,CAII,EAHJ,mBAAA,CAAmE,GAAnE,EAAmE,IAAnE,EAAmE,CAAhE,WAAgE,E,kCAA3B,KAAA,CAAA,IAAA,CAAK,e,GAAe;AAAA;GAAO,CAAnE,CAGI,EAFJ,mBAAA,CAAyD,GAAzD,EAAyD,IAAzD,EAAyD,CAAtD,WAAsD,E,kCAAtB,KAAA,CAAA,IAAA,CAAK,U,GAAU;AAAA;GAAO,CAAzD,CAEI,EADJ,mBAAA,CAA2D,GAA3D,EAA2D,IAA3D,EAA2D,CAAxD,WAAwD,E,kCAAvB,KAAA,CAAA,IAAA,CAAK,W,GAAW;AAAA;GAAO,CAA3D,CACI,CAfN,CAgBI,CAjBN,CA6BI,EAVJ,mBAAA,CASM,KATN,EAAA,WAAA,EASM,CARJ,mBAAA,CAOM,KAPN,EAAA,WAAA,EAOM,CANJ,WAMI,EAJJ,mBAAA,CAAiE,GAAjE,EAAiE,IAAjE,EAAiE,CAA9D,WAA8D,E,kCAA1B,KAAA,CAAA,IAAA,CAAK,c,GAAc;AAAA;GAAO,CAAjE,CAII,EAHJ,mBAAA,CAA+C,GAA/C,EAA+C,IAA/C,EAA+C,CAA5C,WAA4C,E,kCAAjB,KAAA,CAAA,IAAA,CAAK,K,GAAK;AAAA;GAAO,CAA/C,CAGI,EAFJ,mBAAA,CAA+C,GAA/C,EAA+C,IAA/C,EAA+C,CAA5C,WAA4C,E,kCAAjB,KAAA,CAAA,IAAA,CAAK,K,GAAK;AAAA;GAAO,CAA/C,CAEI,EADJ,mBAAA,CAAmD,GAAnD,EAAmD,IAAnD,EAAmD,CAAhD,WAAgD,E,kCAAnB,KAAA,CAAA,IAAA,CAAK,O,GAAO;AAAA;GAAO,CAAnD,CACI,CAPN,CAQI,CATN,CAUI,CA3CN,CA4CI,CA7CN,C","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"columns is-multiline\">\r\n      <div class=\"column is-12\">\r\n        <h1 class=\"title\">{{ lead.company }}</h1>\r\n        <div class=\"buttons\">\r\n          <router-link\r\n            :to=\"{ name: 'EditLead', params: { id: $route.params.id } }\"\r\n            class=\"button is-success\"\r\n            >Edit</router-link\r\n          >\r\n          <button class=\"button is-warning\" @click=\"convertToClient\">\r\n            Convert to client\r\n          </button>\r\n        </div>\r\n      </div>\r\n      <div class=\"column is-6\">\r\n        <div class=\"box\">\r\n          <h2 class=\"subtitle\">Details:</h2>\r\n          <template v-if=\"lead.assigned_to\">\r\n            <p>\r\n              <strong>Assigned to: </strong>\r\n              {{ lead.assigned_to.first_name }}\r\n              {{ lead.assigned_to.last_name }}\r\n            </p>\r\n          </template>\r\n          <p><strong>Status: </strong>{{ lead.status }}</p>\r\n          <p><strong>Priority: </strong>{{ lead.priority }}</p>\r\n          <p><strong>Confidence: </strong>{{ lead.confidence }}</p>\r\n          <p><strong>Estimated Value: </strong>{{ lead.estimated_value }}</p>\r\n          <p><strong>Created at: </strong>{{ lead.created_at }}</p>\r\n          <p><strong>Modified at: </strong>{{ lead.modified_at }}</p>\r\n        </div>\r\n      </div>\r\n\r\n      <div class=\"column is-6\">\r\n        <div class=\"box\">\r\n          <h2 class=\"subtitle\">Contact Information:</h2>\r\n\r\n          <p><strong>Contact Person: </strong>{{ lead.contact_person }}</p>\r\n          <p><strong>Email: </strong>{{ lead.email }}</p>\r\n          <p><strong>Phone: </strong>{{ lead.phone }}</p>\r\n          <p><strong>Website: </strong>{{ lead.website }}</p>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { toast } from \"bulma-toast\";\r\nexport default {\r\n  name: \"Lead\",\r\n  data() {\r\n    return {\r\n      lead: {},\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getLead();\r\n  },\r\n  methods: {\r\n    async getLead() {\r\n      this.$store.commit(\"setIsLoading\", true);\r\n\r\n      const leadId = this.$route.params.id;\r\n\r\n      await axios\r\n        .get(`/api/v1/leads/${leadId}/`)\r\n        .then((response) => {\r\n          this.lead = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n\r\n      this.$store.commit(\"setIsLoading\", false);\r\n    },\r\n    async convertToClient() {\r\n      this.$store.commit(\"setIsLoading\", true);\r\n      const leadID = this.$route.params.id;\r\n      const data = {\r\n        lead_id: leadID,\r\n      };\r\n      await axios\r\n        .post(`/api/v1/convert_lead_to_client/`, data)\r\n        .then((response) => {\r\n          toast({\r\n            message: \"Converted!\",\r\n            type: \"is-success\",\r\n            dismissible: true,\r\n            pauseOnHover: true,\r\n            duration: 2000,\r\n            position: \"bottom-right\",\r\n          });\r\n          this.$router.push(\"/dashboard/clients\");\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n      this.$store.commit(\"setIsLoading\", false);\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":""}]}