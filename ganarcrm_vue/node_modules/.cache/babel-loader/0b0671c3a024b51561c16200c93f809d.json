\r\n        .post(\"/api/v1/notes/\", note)\r\n        .then((response) => {\r\n          toast({\r\n            message: \"Note added!\",\r\n            type: \"is-success\",\r\n            dismissible: true,\r\n            pauseOnHover: true,\r\n            duration: 2000,\r\n            position: \"bottom-right\",\r\n          });\r\n          this.$router.push({\r\n            name: \"Client\",\r\n            params: { id: this.$route.params.id },\r\n          });\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n      this.$store.commit(\"setIsLoading\", false);\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":""}]}