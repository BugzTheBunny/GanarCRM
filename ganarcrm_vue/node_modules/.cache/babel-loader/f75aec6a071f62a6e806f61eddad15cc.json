eads() {\r\n      this.$store.commit(\"setIsLoading\", true);\r\n\r\n      await axios\r\n        .get(\"/api/v1/leads/\")\r\n        .then((response) => {\r\n          this.leads = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n\r\n      this.$store.commit(\"setIsLoading\", false);\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":""}]}