T,KAAA,CAAA,K,GAAK,M;;AAAI,GAAnD,E,IAAA,E;;AAAA,G,iBAA0C,KAAA,CAAA,K,GACtC,CAFN,CAGI,CALN,C,EAMA,mBAAA,CAKM,KALN,EAAA,WAAA,EAKM,CAJJ,WAII,EAHJ,mBAAA,CAEM,KAFN,EAAA,WAAA,EAEM,C,gBADJ,mBAAA,CAAqD,OAArD,EAAqD;AAA9C,IAAA,IAAI,EAAC,MAAyC;AAAlC,IAAA,KAAK,EAAC,OAA4B;;aAAX,KAAA,CAAA,O,GAAO,M;;AAAI,GAArD,E,IAAA,E;;AAAA,G,iBAA0C,KAAA,CAAA,O,GACtC,CAFN,CAGI,CALN,C,EAMA,W,CA/BF,E;;AAAA,GAqCI,CAtCN,CAuCI,CA5CN,CA6CI,CA9CN,C","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"columns is-multiline\">\r\n      <div class=\"column is-12\">\r\n        <h1 class=\"title\">Add Client</h1>\r\n      </div>\r\n\r\n      <div class=\"column is-12\">\r\n        <form @submit.prevent=\"submitForm\">\r\n          <div class=\"field\">\r\n            <label>Name</label>\r\n            <div class=\"control\">\r\n              <input type=\"text\" class=\"input\" v-model=\"name\" />\r\n            </div>\r\n          </div>\r\n          <div class=\"field\">\r\n            <label>Contact Person</label>\r\n            <div class=\"control\">\r\n              <input type=\"text\" class=\"input\" v-model=\"contact_person\" />\r\n            </div>\r\n          </div>\r\n          <div class=\"field\">\r\n            <label>Email</label>\r\n            <div class=\"control\">\r\n              <input type=\"email\" class=\"input\" v-model=\"email\" />\r\n            </div>\r\n          </div>\r\n          <div class=\"field\">\r\n            <label>Phone</label>\r\n            <div class=\"control\">\r\n              <input type=\"text\" class=\"input\" v-model=\"phone\" />\r\n            </div>\r\n          </div>\r\n          <div class=\"field\">\r\n            <label>Website</label>\r\n            <div class=\"control\">\r\n              <input type=\"text\" class=\"input\" v-model=\"website\" />\r\n            </div>\r\n          </div>\r\n          <div class=\"field\">\r\n            <div class=\"control\">\r\n              <button class=\"button is-success is-fullwidth\">Submit</button>\r\n            </div>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport axios from \"axios\";\r\nimport { toast } from \"bulma-toast\";\r\n\r\nexport default {\r\n  name: \"AddClient\",\r\n  data() {\r\n    return {\r\n      name: \"\",\r\n      contact_person: \"\",\r\n      email: \"\",\r\n      phone: \"\",\r\n      website: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    async submitForm() {\r\n      this.$store.commit(\"setIsLoading\", true);\r\n      const client = {\r\n        name: this.name,\r\n        email: this.email,\r\n        contact_person: this.contact_person,\r\n        phone: this.phone,\r\n        website: this.website,\r\n      };\r\n      await axios\r\n        .post(\"/api/v1/clients/\", client)\r\n        .then((response) => {\r\n          toast({\r\n            message: \"Client added!\",\r\n            type: \"is-success\",\r\n            dismissible: true,\r\n            pauseOnHover: true,\r\n            duration: 2000,\r\n            position: \"bottom-right\",\r\n          });\r\n          this.$router.push(\"/dashboard/clients/\");\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n      this.$store.commit(\"setIsLoading\", false);\r\n    },\r\n  },\r\n};\r\n</script>"],"sourceRoot":""}]}