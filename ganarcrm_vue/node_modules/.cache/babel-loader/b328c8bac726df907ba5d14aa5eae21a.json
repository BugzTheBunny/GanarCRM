A;AAAA;AAAA;AAcd,KAfI;AAiBC,IAAA,UAjBD,wBAiBa;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AACR,gBAAA,MADQ,GACC,MAAI,CAAC,MAAL,CAAY,MAAZ,CAAmB,EADpB;;AAEd,gBAAA,MAAI,CAAC,MAAL,CAAY,MAAZ,CAAmB,cAAnB,EAAkC,IAAlC;;AACA,gBAAA,KAAI,CACH,KADD,yBACwB,MADxB,QACkC,MAAI,CAAC,IADvC,EAEC,IAFD,CAEM,UAAA,QAAO,EAAK;AACd,kBAAA,KAAK,CAAC;AACF,oBAAA,OAAO,EAAE,mBADP;AAEF,oBAAA,IAAI,EAAC,YAFH;AAGF,oBAAA,WAAW,EAAC,IAHV;AAIF,oBAAA,YAAY,EAAC,IAJX;AAKF,oBAAA,QAAQ,EAAC,IALP;AAMF,oBAAA,QAAQ,EAAC;AANP,mBAAD,CAAL;;AAQA,kBAAA,MAAI,CAAC,OAAL,CAAa,IAAb,4BAAsC,MAAtC;AACH,iBAZD,EAYG,KAZH,CAYS,UAAA,KAAI,EAAK;AACd,kBAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACH,iBAdD;;AAeA,gBAAA,MAAI,CAAC,MAAL,CAAY,MAAZ,CAAmB,cAAnB,EAAkC,KAAlC;;AAlBc;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAmBlB;AApCK;AAVE,CAAf","sourcesContent":["<template>\r\n    <div class=\"container\">\r\n        <div class=\"columns is-multiline\">\r\n            <div class=\"column is-12\">\r\n                <h1 class=\"title\">Edit {{ lead.title }}</h1>\r\n            </div>\r\n\r\n            <div class=\"column is-12\">\r\n                <form @submit.prevent=\"submitForm\">\r\n                    <div class=\"field\">\r\n                        <label>Company</label>\r\n                        <div class=\"control\">\r\n                            <input type=\"text\" class=\"input\" v-model=\"lead.company\">\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"field\">\r\n                        <label>Contact Person</label>\r\n                        <div class=\"control\">\r\n                            <input type=\"text\" class=\"input\" v-model=\"lead.contact_person\">\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"field\">\r\n                        <label>Email</label>\r\n                        <div class=\"control\">\r\n                            <input type=\"email\" class=\"input\" v-model=\"lead.email\">\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"field\">\r\n                        <label>Phone</label>\r\n                        <div class=\"control\">\r\n                            <input type=\"text\" class=\"input\" v-model=\"lead.phone\">\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"field\">\r\n                        <label>Website</label>\r\n                        <div class=\"control\">\r\n                            <input type=\"text\" class=\"input\" v-model=\"lead.website\">\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"field\">\r\n                        <label>Confidence</label>\r\n                        <div class=\"control\">\r\n                            <input type=\"number\" class=\"input\" v-model=\"lead.confidence\">\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"field\">\r\n                        <label>Estimated Value</label>\r\n                        <div class=\"control\">\r\n                            <input type=\"number\" class=\"input\" v-model=\"lead.estimated_value\">\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"field\">\r\n                        <label>Status</label>\r\n                        <div class=\"control\">\r\n                            <div class=\"select\">\r\n                                <select v-model=\"lead.status\">\r\n                                    <option value=\"new\">New</option>\r\n                                    <option value=\"contacted\">Contacted</option>\r\n                                    <option value=\"inprogress\">In Progress</option>\r\n                                    <option value=\"lost\">Lost</option>\r\n                                    <option value=\"won\">Won</option>\r\n                                </select>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"field\">\r\n                        <label>Priority</label>\r\n                        <div class=\"control\">\r\n                            <div class=\"select\">\r\n                                <select v-model=\"lead.priority\">\r\n                                    <option value=\"low\">Low</option>\r\n                                    <option value=\"medium\">Medium</option>\r\n                                    <option value=\"high\">High</option>\r\n                                </select>\r\n                            </div>\r\n                        </div>\r\n                    </div>\r\n                    <div class=\"field\">\r\n                        <div class=\"control\">\r\n                            <button class=\"button is-success is-fullwidth\">Update</button>\r\n                        </div>\r\n                    </div>\r\n                </form>\r\n            </div>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from 'axios'\r\nimport {toast} from 'bulma-toast'\r\n\r\nexport default {\r\n    name:'EditLead',\r\n    data() {\r\n        return {\r\n            lead:{}\r\n        }\r\n    },\r\n    mounted() {\r\n        this.getLead()\r\n    },\r\n    methods: {\r\n        async getLead(){\r\n            this.$store.commit('setIsLoading',true)\r\n\r\n            const leadID = this.$route.params.id\r\n\r\n            axios\r\n            .get(`api/v1/leads/${leadID}`)\r\n            .then(response => {\r\n                this.lead = response.data\r\n            }).catch(error => {\r\n                console.log(error)\r\n            })\r\n\r\n            this.$store.commit('setIsLoading',false)\r\n        },\r\n\r\n        async submitForm(){\r\n            const leadID = this.$route.params.id\r\n            this.$store.commit('setIsLoading',true)\r\n            axios\r\n            .patch(`/api/v1/leads/${leadID}/`,this.lead)\r\n            .then(response => {\r\n                toast({\r\n                    message: 'Lead was updated!',\r\n                    type:'is-success',\r\n                    dismissible:true,\r\n                    pauseOnHover:true,\r\n                    duration:2000,\r\n                    position:'bottom-right'\r\n                })\r\n                this.$router.push(`/dashboard/leads/${leadID}`)\r\n            }).catch(error => {\r\n                console.log(error)\r\n            })\r\n            this.$store.commit('setIsLoading',false)\r\n        }\r\n    },\r\n}\r\n</script>"],"sourceRoot":""}]}