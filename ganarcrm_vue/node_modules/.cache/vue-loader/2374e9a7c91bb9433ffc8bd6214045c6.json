         <router-link\r\n                      :to=\"{ name: 'Client', params: { id: client.id } }\"\r\n                      class=\"button is-success\"\r\n                      >View</router-link\r\n                    >\r\n                    <router-link\r\n                      :to=\"{ name: 'EditClient', params: { id: client.id } }\"\r\n                      class=\"button is-success\"\r\n                      >Edit</router-link\r\n                    >\r\n                  </div>\r\n                </td>\r\n              </tr>\r\n            </tbody>\r\n          </table>\r\n        </template>\r\n        <template v-else>\r\n          <h1>You don't have any clients yet.</h1>\r\n        </template>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n\r\n<script>\r\nimport axios from \"axios\";\r\n\r\nexport default {\r\n  name: \"Clients\",\r\n  data() {\r\n    return {\r\n      clients: [],\r\n    };\r\n  },\r\n\r\n  mounted() {\r\n    this.getClients();\r\n  },\r\n\r\n  methods: {\r\n    async getClients() {\r\n      this.$store.commit(\"setIsLoading\", true);\r\n\r\n      // await axios\r\n      //   .get(\"/api/v1/clients/\")\r\n      //   .then((response) => {\r\n      //     this.clients = response.data;\r\n      //   })\r\n      //   .catch((error) => {\r\n      //     console.log(error);\r\n      //   });\r\n\r\n      this.$store.commit(\"setIsLoading\", false);\r\n    },\r\n  },\r\n};\r\n</script>"]}]}