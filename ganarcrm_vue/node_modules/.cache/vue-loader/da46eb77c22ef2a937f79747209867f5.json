AAC;MACR,CAAC,CAAC,CAAC,CAAC,CAAC;IACP,CAAC,CAAC,CAAC,CAAC,CAAC;EACP,CAAC,CAAC,CAAC,CAAC,CAAC","file":"D:/Projects/GanarCRM/ganarcrm_vue/src/views/dashboard/teams/EditUser.vue","sourceRoot":"","sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"columns is-multiline\">\r\n      <div class=\"column is-12\">\r\n        <h1 class=\"title\">Edit member</h1>\r\n      </div>\r\n\r\n      <div class=\"column is-12\">\r\n        <form @submit.prevent=\"submitForm\">\r\n          <div class=\"field\">\r\n            <label>First name</label>\r\n            <div class=\"control\">\r\n              <input type=\"text\" class=\"input\" v-model=\"user.first_name\" />\r\n            </div>\r\n          </div>\r\n          <div class=\"field\">\r\n            <label>Last name</label>\r\n            <div class=\"control\">\r\n              <input type=\"text\" class=\"input\" v-model=\"user.last_name\" />\r\n            </div>\r\n          </div>\r\n          <div class=\"field\">\r\n            <div class=\"control\">\r\n              <button class=\"button is-success is-fullwidth\">Update</button>\r\n            </div>\r\n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { toast } from \"bulma-toast\";\r\n\r\nexport default {\r\n  name: \"EditUser\",\r\n  data() {\r\n    return {\r\n      user: {},\r\n    };\r\n  },\r\n  mounted() {\r\n    this.getUser();\r\n  },\r\n  methods: {\r\n    async getUser() {\r\n      this.$store.commit(\"setIsLoading\", true);\r\n\r\n      const userID = this.$route.params.id;\r\n\r\n      axios\r\n        .get(`api/v1/teams/member/${userID}`)\r\n        .then((response) => {\r\n          this.user = response.data;\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n\r\n      this.$store.commit(\"setIsLoading\", false);\r\n    },\r\n\r\n    async submitForm() {\r\n      this.$store.commit(\"setIsLoading\", true);\r\n\r\n      const userID = this.$route.params.id;\r\n      axios\r\n        .put(`/api/v1/teams/member/${userID}/`, this.user)\r\n        .then((response) => {\r\n          toast({\r\n            message: \"The user was updated!\",\r\n            type: \"is-success\",\r\n            dismissible: true,\r\n            pauseOnHover: true,\r\n            duration: 2000,\r\n            position: \"bottom-right\",\r\n          });\r\n          this.$router.push({ name: \"MyAccount\" });\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n      this.$store.commit(\"setIsLoading\", false);\r\n    },\r\n  },\r\n};\r\n</script>"]}]}