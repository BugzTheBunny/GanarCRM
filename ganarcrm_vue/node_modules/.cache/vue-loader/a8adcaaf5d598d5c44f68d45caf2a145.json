n          </div>\r\n        </form>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport axios from \"axios\";\r\nimport { toast } from \"bulma-toast\";\r\nexport default {\r\n  name: \"AddTeam\",\r\n  data() {\r\n    return {\r\n      name: \"\",\r\n    };\r\n  },\r\n  methods: {\r\n    async submitForm() {\r\n      this.$store.commit(\"setIsLoading\", true);\r\n\r\n      const team = {\r\n        name: this.name,\r\n      };\r\n\r\n      await axios\r\n        .post(\"api/v1/teams/\", team)\r\n        .then((response) => {\r\n          toast({\r\n            message: \"Team Created!\",\r\n            type: \"is-success\",\r\n            dismissible: true,\r\n            pauseOnHover: true,\r\n            duration: 2000,\r\n            position: \"bottom-right\",\r\n          });\r\n          this.$store.commit(\"setTeam\", {\r\n            id: response.data.id,\r\n            name: response.data.name,\r\n          });\r\n\r\n          this.$router.push(\"dashboard\");\r\n        })\r\n        .catch((error) => {\r\n          console.log(error);\r\n        });\r\n\r\n      this.$store.commit(\"setIsLoading\", false);\r\n    },\r\n  },\r\n};\r\n</script>"]}]}